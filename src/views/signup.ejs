        <div class="top-spacer"> </div>
      </div>
    </div> 
  </div><!-- /cont -->
  
</div>


<div class="container">
  <div class="row">
    
    <div class="col-md-12"> 
      
      <div class="panel">
        <div class="panel-body">
          
          
     
       <!--FORM-->

       <form action="/login" class="" onsubmit="return validate(),passwordvalidate(),match(),namevalidate(),phonenovalidate() ">
        <h2> Create your account</h2>
        <br>
       <div class="logincont">
        <label>Name</label><br>
          
          <input type="text" class="form-control" placeholder="Enter your full name" id="username">
      </div><label for="error4" id="error4"></label><br>
    
        <div class="logincont">
            <label>Email-id</label><br>
            <input type="text" class="form-control" placeholder="Enter your Email-id" id="emailid">
        </div><label for="error" id="error"></label><br>

        <div class="logincont">
          <label>Phone no :</label><br>
          <input type="text" class="form-control" placeholder="Enter your 10 digit phone number" id="phoneno">
      </div> <label for="error5" id="error5"></label><br>

         <div class="logincont">
            <label>Set Password</label><br>
            <div id="passwordHelpBlock" class="form-text">
              Your password must be at least 8 characters long, contain at least one uppercase letter,lowercase letter, number, special character and must not contain spaces.
            </div><br>   
            <input type="password" class="form-control" placeholder="Password" id="pwd">
        </div> <label for="error2" id="error2"></label><br>

        <div class="logincont">
          <label>Confirm Password</label><br>
              
          <input type="password"  class="form-control" placeholder="Re-type your password" id="pwd2">
      </div><br>  <label for="error3" id="error3"></label><br>
    
        
       <!--
        <input type="checkbox">
        <label>Remember password?</label><br><br>-->
        <div>
            <button id="submit" class="btn btn-primary" onsubmit=""  >Create Account</button>
        </div><br>
        <div class="loginmisc">
            <h5>Already have an account?</h5>
            <a href="/login">Log In!</a>
        </div>

    </form>

        
          
          
          
        
          
        </div>
      </div>
                                                                                       
	                                                
                                                      
   	</div><!--/col-12-->
